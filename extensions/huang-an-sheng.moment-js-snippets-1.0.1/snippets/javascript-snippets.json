{
    "Require moment.js": {
        "prefix": "moment.require",
        "body": [
            "const moment = require('moment');"
        ],
        "description": "Require moment.js"
    },
    "Moment format": {
        "prefix": "moment.format",
        "body": [
            "moment().format(${1:})"
        ],
        "description": "Moment format"
    },
    "Moment formatConvert": {
        "prefix": "moment.formatConvert",
        "body": [
            "moment(${1:'2012/02/22'}, ${2:'YYYY/MM/DD'}).format(${3:'Do MMM YYYY dddd'})"
        ],
        "description": "Convert date format to another format"
    },
    "moment format MMMM Do YYYY, h mm ss a": {
        "prefix": "moment.format.MMMMDoYYYY,hmmssa",
        "body": [
            "moment().format('MMMM Do YYYY, h:mm:ss a')"
        ],
        "description": "Get format: February 22nd 2012, 12:00:00 am"
    },
    "moment format MMMM Do YYYY, dddd, h mm ss a": {
        "prefix": "moment.format.MMMMDoYYYY,dddd,hmmssa",
        "body": [
            "moment().format('MMMM Do YYYY, dddd, h:mm:ss a')"
        ],
        "description": "Get format: February 22nd 2012, Wednesday, 12:00:00 am"
    },
    "moment format dddd": {
        "prefix": "moment.format.dddd",
