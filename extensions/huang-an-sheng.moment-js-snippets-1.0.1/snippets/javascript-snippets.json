        "prefix": "moment.addCalendar",
        "body": [
            "moment().add(${1:10}, 'days').calendar()"
        ],
        "description": "moment add calendar"
    },
    "moment subtract calendar": {
        "prefix": "moment.subtractCalendar",
        "body": [
            "moment().subtract(${1:10}, 'days').calendar()"
        ],
        "description": "moment subtract calendar"
    },
    "moment toArray": {
        "prefix": "moment.toArray",
        "body": [
            "moment().toArray()"
        ],
        "description": "Returns an array that mirrors the parameters from new Date()"
    },
    "moment unix timestamp(milliseconds)": {
        "prefix": "moment.valueOf",
        "body": [
            "moment().valueOf()"
        ],
        "description": "Returns the number of milliseconds since the Unix Epoch"
    },
    "moment unix timestamp(seconds)": {
        "prefix": "moment.unix",
        "body": [
            "moment().unix()"
        ],
        "description": "Returns a Unix timestamp"
    }
